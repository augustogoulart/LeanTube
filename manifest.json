{
    "name": "LeanTube",
    "description": "No home page. No recommendations.",
    "version": "0.1",
    "manifest_version": 3,
    "content_scripts": [{
        "matches": ["https://www.youtube.com/"],
        "js": ["home.js"]
    },
    {
        "matches": ["https://www.youtube.com/watch?*"],
        "js": ["watch.js"]
    }
]
}